{
    "name" : "example",
    "active" : true,
    "proxy" : {
        "preserve_host" : false,
        "listen_path" : "/example/*",
        "upstreams" : {
            "balancing": "roundrobin",
            "targets": [
                {"target": "http://service1:8080/"}
            ]
        },
        "strip_path" : false,
        "append_path" : false,
        "methods" : ["GET"]
    },
    "health_check": {
        "url": "http://service1:8080/status",
        "timeout": 3
    }
}
